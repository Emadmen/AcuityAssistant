{
    "name": "acuityassistant",
    "version": "0.1.0",
    "private": true,
    "main": "dist/index",
    "types": "dist/index",
    "files": [
        "dist",
        "README.md"
    ],
    "dependencies": {
        "@fluentui/react-components": "^9.72.4",
        "@fluentui/react-icons": "^2.0.313",
        "@microsoft/teams-js": "^2.46.0",
        "@microsoft/teams.api": "^2.0.0",
        "@microsoft/teams.apps": "^2.0.0",
        "@microsoft/teams.cards": "^2.0.0",
        "@microsoft/teams.client": "^2.0.0",
        "@microsoft/teams.common": "^2.0.0",
        "@microsoft/teams.dev": "^2.0.0",
        "@microsoft/teams.graph": "^2.0.0",
        "@microsoft/teams.graph-endpoints": "^2.0.0",
        "react": "^19.0.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.0",
        "@types/jest": "^30.0.0",
        "@types/node": "^22.5.4",
        "@types/react": "^19.2.2",
        "@types/react-dom": "^19.2.2",
        "@vitejs/plugin-react": "^4.3.4",
        "dotenv": "^16.4.5",
        "env-cmd": "^11.0.0",
        "jsdom": "^27.1.0",
        "nodemon": "^3.1.4",
        "rimraf": "^6.0.1",
        "tsup": "^8.4.0",
        "typescript": "^5.4.5",
        "vite": "^6.2.0",
        "vitest": "^4.0.7",
        "serve": "^14.2.5"
    },
    "scripts": {
        "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run start",
        "clean": "rimraf ./dist",
        "start": "nodemon",
        "start:tab": "serve -s dist/client -l 53000",
        "prestart": "npm run build:frontend",
        "build": "tsup && npm run build:frontend",
        "build:frontend": "vite build --outDir dist/client",
        "test": "vitest",
        "test:run": "vitest run",
        "test:watch": "vitest --watch",
        "test:ui": "vitest --ui"
    }
}